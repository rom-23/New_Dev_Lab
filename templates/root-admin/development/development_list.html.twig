{%- set navbar = '/navbars/admin_navbar.html.twig' -%}
{% extends navbar %}
{% block body %}

    <div class="container">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>ID</th>
            <th>Titre</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for dev in developments %}
            <tr>
                <td>{{ dev.id }}</td>
                <td>{{ dev.title }}</td>
                <td>
                    <a href="{{ path('root_admin_dev_edit', {id: dev.id}) }}" class="btn btn-secondary">Editer</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
{% endblock %}

